{
    "name": "IBM MQ Queue Status",
    "description": null,
    "pages": [
      {
        "name": "IBM MQ Queue Status",
        "description": null,
        "widgets": [
          {
            "visualization": {
              "id": "viz.line"
            },
            "layout": {
              "column": 1,
              "row": 1,
              "height": 6,
              "width": 8
            },
            "title": "% Queue Depth",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM MQQueueSample SELECT average(qDepthPercent) SINCE 30 MINUTES AGO UNTIL 2 minutes ago FACET qName TIMESERIES"
                }
              ]
            },
            "linkedEntityGuids": null
          },
          {
            "visualization": {
              "id": "viz.table"
            },
            "layout": {
              "column": 9,
              "row": 1,
              "height": 6,
              "width": 4
            },
            "title": "Queue Depth",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM MQQueueSample SELECT latest(qDepth) AS `Current Q Depth`, average(qDepth) AS `Average Q Depth` FACET qName"
                }
              ]
            },
            "linkedEntityGuids": null
          },
          {
            "visualization": {
              "id": "viz.table"
            },
            "layout": {
              "column": 1,
              "row": 7,
              "height": 3,
              "width": 12
            },
            "title": "Latest Queue Statistics",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM MQQueueSample SELECT latest(openInputCount), latest(openOutputCount), latest(msgEnqCount), latest(msgDeqCount), latest(qDepth), latest(highQDepth) FACET qName"
                }
              ]
            },
            "linkedEntityGuids": null
          },
          {
            "visualization": {
              "id": "viz.table"
            },
            "layout": {
              "column": 1,
              "row": 10,
              "height": 3,
              "width": 12
            },
            "title": "Average Queue Statistics",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM MQQueueSample SELECT average(openInputCount), average(openOutputCount), average(msgEnqCount), average(msgDeqCount), average(qDepth), average(qDepthPercent) FACET qName"
                }
              ]
            },
            "linkedEntityGuids": null
          }
        ]
      }
    ]
  }
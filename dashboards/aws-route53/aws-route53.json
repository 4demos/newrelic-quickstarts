{
    "name": "Aws route53",
    "description": "",
    "pages": [
        {
            "name": "Aws route53",
            "description": "",
            "widgets": [
                {
                    "title": "Health check status",
                    "layout": {
                        "height": 3,
                        "width": 8,
                        "row": 1,
                        "column": 1
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "select min(`aws.route53.HealthCheckStatus`) FROM Metric TIMESERIES 1 minute since 1 hour ago until 5 minutes ago facet entity.name"
                            }
                        ]
                    }
                },
                {
                    "title": "",
                    "layout": {
                        "height": 6,
                        "width": 4,
                        "row": 1,
                        "column": 9
                    },
                    "visualization": {
                        "id": "viz.markdown"
                    },
                    "rawConfiguration": {
                        "text": "::: beyond\n![Amazon Route53 Icon](https://integrations.nr-assets.net/providers/aws_route53.png) **AWS Route 53** \n\nAmazon Route 53 is a highly available and scalable cloud Domain Name System (DNS) web service.\n\nEach chart is reporting every minute.\n\n\n:::"
                    }
                },
                {
                    "title": "Successful health checks (%)",
                    "layout": {
                        "height": 3,
                        "width": 8,
                        "row": 4,
                        "column": 1
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "select average(`aws.route53.HealthCheckPercentageHealthy`) FROM Metric TIMESERIES 1 minute since 1 hour ago until 5 minutes ago facet entity.name"
                            }
                        ]
                    }
                },
                {
                    "title": "Connection time (ms)",
                    "layout": {
                        "height": 3,
                        "width": 4,
                        "row": 7,
                        "column": 1
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "select average(`aws.route53.ConnectionTime`) FROM Metric TIMESERIES 1 minute since 1 hour ago until 5 minutes ago facet entity.name"
                            }
                        ]
                    }
                },
                {
                    "title": "Time to first byte (ms)",
                    "layout": {
                        "height": 3,
                        "width": 4,
                        "row": 7,
                        "column": 5
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "select average(`aws.route53.TimeToFirstByte`) FROM Metric TIMESERIES 1 minute since 1 hour ago until 5 minutes ago facet entity.name"
                            }
                        ]
                    }
                },
                {
                    "title": "Ssl handshake time (ms)",
                    "layout": {
                        "height": 3,
                        "width": 4,
                        "row": 7,
                        "column": 9
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "select average(`aws.route53.SSLH shakeTime`) FROM Metric TIMESERIES 1 minute since 1 hour ago until 5 minutes ago facet entity.name"
                            }
                        ]
                    }
                },
                {
                    "title": "Route 53 service limit usage",
                    "layout": {
                        "height": 3,
                        "width": 12,
                        "row": 10,
                        "column": 1
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT max(`aws.trustedadvisor.limitUsage`) FROM Metric WHERE aws.trustedadvisor.service='Route53' FACET aws.trustedadvisor.limitName, aws.region TIMESERIES SINCE 1 week ago LIMIT 1000"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
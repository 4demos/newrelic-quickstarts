          "title": "Number of Brokers",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "dataFormatters": [
              {
                "name": "Last Update",
                "type": "date"
              }
            ],
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [],
                "query": "FROM Metric SELECT latest(kafka.brokers) as 'Kafka Brokers',latest(timestamp) as 'Last Update' where kafka.brokers IS NOT NULL"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },

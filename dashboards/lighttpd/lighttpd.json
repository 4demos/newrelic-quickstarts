              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "SELECT average(apm.service.memory.physical) * rate(count(apm.service.instance.count), 1 minute) / 1000 as memoryUsage FROM Metric WHERE (host LIKE '%lighttpd-VM%') LIMIT 20 SINCE 1800 seconds AGO TIMESERIES "

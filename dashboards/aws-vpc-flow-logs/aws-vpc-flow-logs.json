                        "id": "viz.area"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT sum(`provider.bytes`) / 1024 / 1024 from PrivateNetworkSample where provider='VpcNetworkInterface' TIMESERIES auto since 1 day ago facet `provider.subnetId`"
                            }

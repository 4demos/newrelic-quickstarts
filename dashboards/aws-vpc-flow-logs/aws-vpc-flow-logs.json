            "widgets": [
                {
                    "title": "Traffic by vpc id",
                    "layout": {
                        "height": 3,
                        "width": 4,
                        "row": 1,
                        "column": 1
                    },
                    "visualization": {
                        "id": "viz.area"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT sum(`provider.bytes`) / 1024 / 1024 from PrivateNetworkSample where provider='VpcNetworkInterface' TIMESERIES auto since 1 day ago facet `provider.vpcId`"
                            }
                        ]
                    }
                },
                {
                    "title": "Traffic by subnet id",
                    "layout": {

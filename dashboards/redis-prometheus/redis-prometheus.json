            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT sum(redis_commands_processed_total)  as 'Commands' FROM Metric  where metricName = 'redis_commands_processed_total' AND instrumentation.name='remote-write' and instance={{instance}} and  prometheus_server={{prometheus_server}} TIMESERIES "
              }
